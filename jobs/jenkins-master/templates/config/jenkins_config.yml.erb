jenkins:
  agentProtocols:
  - "JNLP4-connect"
  - "Ping"
  crumbIssuer:
    standard:
      excludeClientIPFromCrumb: false
<% if_p('labels') do |labels| %>
  labelString: "<%= labels %>"
<% end %>
  mode: <%= p('mode').upcase %>
  numExecutors: <%= p('executors') %>
  remotingSecurity:
    enabled: true
  slaveAgentPort: 50000
unclassified:
  location:
    url: "http://<%= spec.ip %>:<%= p('port') %>"
tool:
  git:
    installations:
    - name: "Default"
      home: "/var/vcap/packages/git-client/bin/git"
  jdk:
    installations:
    - name: "Default"
      home: "/var/vcap/packages/openjdk-8/jre"
